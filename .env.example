
# Optional: per-agent model (use cheaper models for discovery/finder)
# OPENAI_DISCOVERY_MODEL=gpt-4.1-mini
# OPENAI_EMAIL_FINDER_MODEL=gpt-4.1-mini
# OPENAI_WRITER_MODEL=gpt-4.1-mini

# Optional: Outlook session file (default: outlook_session.json)
OUTLOOK_SESSION_PATH=outlook_session.json

# Optional: path to track emailed companies (default: emailed_companies.json)
EMAILED_COMPANIES_PATH=emailed_companies.json

# Required: OpenAI API key for Agents SDK
OPENAI_API_KEY=your-openai-api-key-here


# Gmail: path to OAuth2 credentials JSON (from Google Cloud Console)
# Download as credentials.json and set path, or use default ./credentials.json
#GMAIL_CREDENTIALS_PATH=./credentials.json

OPENAI_DEFAULT_MODEL=gpt-4o-mini
OPENAI_DISCOVERY_MODEL=gpt-4o-mini
OPENAI_EMAIL_FINDER_MODEL=gpt-4o-mini
OPENAI_WRITER_MODEL=gpt-4o-mini
